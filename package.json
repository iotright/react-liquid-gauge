{
  "name": "react-liquid-gauge",
  "version": "1.3.0",
  "description": "React Liquid Gauge component",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "bowersync": "node ./scripts/bowersync",
    "build": "babel --out-dir ./lib ./src",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f lib/*",
    "demo": "http-server -p 8000 docs/",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-liquid-gauge.git"
  },
  "author": "Cheton Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-liquid-gauge/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-liquid-gauge",
  "keywords": [
    "react",
    "react-component",
    "d3",
    "liquid",
    "gauge",
    "chart"
  ],
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@trendmicro/react-buttons": "~1.3.1",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~9.2.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.5",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "coveralls": "~3.1.1",
    "css-loader": "~7.1.2",
    "eslint": "~7.2.0",
    "eslint-loader": "~4.0.2",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-jsx-a11y": "~6.10.2",
    "eslint-plugin-react": "~7.37.2",
    "file-loader": "~6.2.0",
    "html-webpack-plugin": "~5.6.3",
    "http-server": "~14.1.1",
    "nib": "~1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "~4.0.0",
    "stylint": "~1.2.2",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~8.1.1",
    "tap": "~21.0.1",
    "trendmicro-ui": "~0.5.2",
    "url-loader": "~4.1.1",
    "webpack": "~5.96.1",
    "webpack-dev-server": "~5.1.0",
    "which": "~5.0.0"
  }
}
